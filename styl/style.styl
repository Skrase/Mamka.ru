blueRgba(n)
	rgba(52, 77, 118, n)

lightBlueRGBA(n)
	rgba(127, 138, 168, n)

placeholder-color(n)
	&::-webkit-input-placeholder
		color n
	&::-moz-placeholder
		color n
	&:-moz-placeholder
		color n
	&:-ms-input-placeholder
		color n

futura = "FuturaRoundBold", "Roboto", sans-serif

