.checkbox-ui
	display inline-block
	font-weight 300
	cursor pointer
	overflow hidden
	margin 0 0 9px
	line-height 16px
	color blue
	input[type="checkbox"]:checked
		+ div
			font-weight 500
		+ div.checkbox-item:after
			transform translateY(0)
	&:hover
		font-weight 500
		.checkbox-item
			border 1px solid blue
	input
		display none
	div
		display inline-block
		font-size 14px
		line-height 16px
		vertical-align middle
		&.checkbox-item
			width 14px
			height 14px
			border 1px solid topGray
			border-radius 2px
			margin 0 25px 0 0
			vertical-align top
			border 1px solid blue
			position relative
			overflow hidden
			
			&:after
				width 14px
				height 14px
				position absolute
				top 0
				bottom 0
				right 0
				left 0
				margin auto
				background url('data:image/png;base64,') no-repeat center center
				background-size 12px 12px
				transition all .2s ease
				display block
				content ""
				transform translateY(30px)


.radio-ui
	display block
	font-weight 300
	cursor pointer
	overflow hidden
	margin 0 0 9px
	line-height 16px
	color blue
	input[type="radio"]:checked
		+ div
			font-weight 500
		+ div.radio-item:after
			transform translateY(0)
	&:hover
		font-weight 500
		.radio-item
			border 1px solid blue
	input
		display none
	div
		display inline-block
		font-size 14px
		line-height 16px
		vertical-align middle
		&.radio-item
			width 14px
			height 14px
			border 1px solid topGray
			border-radius 2px
			margin 0 25px 0 0
			vertical-align top
			border 1px solid blue
			position relative
			overflow hidden
			background transparent
			border-radius 50%
			&:after
				width 10px
				height 10px
				position absolute
				top 0
				bottom 0
				right 0
				left 0
				margin auto
				background pink
				border-radius 50%
				transition all .2s ease
				display block
				content ""
				transform translateY(30px)

.more-button
