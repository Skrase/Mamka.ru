.added-wrapper
	max-width 1230px
	position fixed
	top 0
	left 0
	right 0
	margin auto
	z-index 6
	display none

#added-block
	position absolute
	right 30px
	top 30px
	width 255px
	.add-cart-block
		background white
		border 1px solid cartBorder
		position relative
		border-radius 2px
		&:before
			display block
			content ""
			background pink
			height 11px
			width 100%
		&:after
			display block
			content ""
			background white
			height 8px
			width 8px
			border 1px solid cartBorder
			position absolute
			right 20px
			top -5px
			transform rotate(45deg)
		img
			display block
			float left
			border 1px solid cartBorder
			border-radius 2px
			margin 0 10px 10px
			width 85px
			height auto
		.add-cart-block-content
			float left
			width calc(100% - 107px)
			.add-cart-block-title
				padding 12px 0
				color blue
				font-size 13px
				line-height 18px
				font-weight 500
			.add-cart-block-price
				color green
				font-size 18px
				line-height 18px
				position absolute
				right 10px
				bottom 10px
				font-weight 500
		.clearfix
			clear both

.owl-controls
	height 0
	.owl-nav
		.owl-prev
			position absolute
			background url("../i/slider-arrow.png") no-repeat center center
			height 32px
			width 18px
			top 0
			bottom 0
			left 10px
			margin auto
		.owl-next
			position absolute
			background url("../i/slider-arrow.png") no-repeat center center
			height 32px
			width 18px
			top 0
			bottom 0
			right 10px
			transform rotate(180deg)
			margin auto
	.owl-dots
		position absolute
		left 10px
		bottom 10px
		height 14px
		.owl-dot
			width 14px
			height 14px
			border-radius 50%
			margin 0 7px 0 0
			background rgba(255,255,255,.3)
			display inline-block
			&:hover, &.active
				background pink


@media screen and (max-width: 767px)
	.owl-controls
		.owl-nav
			.owl-prev
				height 14px
				width 7px
				left 20px
				background-size 100% 100%
			.owl-next
				height 14px
				width 7px
				right 20px
				background-size 100% 100%
		.owl-dots
			left 20px
			bottom 12px
			.owl-dot
				width 5px
				height 5px
				margin 0 2px 0 0
				display inline-block
				&:hover, &.active
					background pink

