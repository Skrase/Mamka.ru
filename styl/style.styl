.tab-content
	display none
	&.active
		display block

input:hover:not(:focus)
	transition all .2s ease-in
	border-color pink!important

input:focus
	outline #a5c7fe auto 5px;


.my-range-w	{
	margin 35px 0 0
	display block
	position relative
	border-radius 0
}
.my-range {
	position: relative;
	width: 100%
	height: 8px;
	background-color: #d1d1d1;
	margin: auto;
	border-radius 0
	.ui-slider-range {
		position: absolute; // important
		z-index: 1;
		background: linear-gradient(to right,  #8697b5 0%,#ceadc2 100%);
		margin: auto;
		height: 100%;
	}
	.ui-slider-handle {
		position: absolute;
		top: -4px;
		border 1px solid #838daa
		z-index: 2;
		width: 15px;
		height: 15px;
		background: #FFF;
		border-radius: 50%;
		outline: none;
		cursor: pointer;
	}
}

.my-range-value
	position absolute
	top -25px
	width 100%
	font-size 14px
	.v-min
		float left
		position relative
		color blue
		&:after
			display block
			content ""
			height 8px
			width 1px
			background topGray
			top 100%
			left 10px
			position absolute
	.v-max
		float right
		position relative
		color blue
		&:after
			display block
			content ""
			height 8px
			width 1px
			background topGray
			top 100%
			right 10px
			position absolute













.count-picker
	line-height 25px
	text-align center
	font-weight 500
	.count-button
		display inline-block
		border none
		background blue
		color white
		width 25px
		height 25px
		border-radius 2px
		cursor pointer
		transition all .2s ease-in
